int main()
{
    int a,b,c;
    cout<<"a = ";
    cin>>a;
    cout<<"b = ";
    cin>>b;
    cout<<"c = ";
    cin>>c;
        if(a == b*-1){
            cout<<c;
        }
        else if(a == c*-1){
            cout<<b;
        }
        else if(b == c*-1){
            cout<<a;
        }
        else cout<<"Жоқ";
    
}
